#: src-en/data/events_special_gb.xml.append:105
msgid ""
"data/events_special_gb.xml.append$//event[@name=\"FR_PROTON_HAUNT_GOZER\"]/choice/event/choice[2]/event/choice[5]/text"
msgstr "艾乐贝妮尔。"

#: src-en/data/events_special_gb.xml.append:107
msgid ""
"data/events_special_gb.xml.append$//event[@name=\"FR_PROTON_HAUNT_GOZER\"]/choice/event/choice[2]/event/choice[5]/event/text"
msgstr "\"你说是我妻子？嗯…既然如此，那就开始你的测试吧！\""

#: src-en/data/events_special_gb.xml.append:109
msgid ""
"data/events_special_gb.xml.append$//event[@name=\"FR_PROTON_HAUNT_GOZER\"]/choice/event/choice[2]/event/choice[5]/event/choice/text"
