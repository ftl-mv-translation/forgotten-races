msgid ""
"data/events_sector_obelisk.xml.append$//event[@name=\"FR_COMMONWEALTH_FORGE_DRONE\"]/beaconType/undiscoveredTooltip"
msgstr "A strange broadcast seems to be coming from this beacon."

#: src-en/data/events_sector_obelisk.xml.append:9
msgid ""
"data/events_sector_obelisk.xml.append$//event[@name=\"FR_COMMONWEALTH_FORGE_DRONE\"]/beaconType/unvisitedTooltip"
