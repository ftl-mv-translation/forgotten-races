#: src-en/data/blueprints.xml.append:6744
msgid ""
"data/blueprints.xml.append$//weaponBlueprint[@name=\"FR_CHARROR_DRONE_BEAM\"]/title"
msgstr "Отладочный лучевой дрон"

#: src-en/data/blueprints.xml.append:6782
msgid ""
"data/blueprints.xml.append$//weaponBlueprint[@name=\"FR_LEO_BATTERY\"]/title"
msgstr "Блок питания Лео"

#: src-en/data/blueprints.xml.append:6783
msgid ""
"data/blueprints.xml.append$//weaponBlueprint[@name=\"FR_LEO_BATTERY\"]/short"
msgstr "Б. питания Лео"

#: src-en/data/blueprints.xml.append:6784
msgid ""
"data/blueprints.xml.append$//weaponBlueprint[@name=\"FR_LEO_BATTERY\"]/desc"
msgstr ""
"В отличие от других орудий с блоком питания, этот посылает во врага "
