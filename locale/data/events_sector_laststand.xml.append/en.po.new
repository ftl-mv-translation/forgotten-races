"data/events_sector_laststand.xml.append$//mod:findName[@name=\"FLAGSHIP_JERRY\"]/mod:findLike/mod:findLike/mod:findLike/mod:findLike/mod:findLike/mod:findLike/mod:findLike/mod:findLike/mod:findLike/mod:findLike/mod-"
