#: src-en/data/events_sector_showdown.xml.append:149
msgid ""
"data/events_sector_showdown.xml.append$//event[@name=\"FR_TRUE_VICTORY_CREW_CYBERPONY\"]/text"
msgstr ""
"\"Neigggh! Horsie [happy]! Horsie [horsie] helped -helped-! [Break bomb]! "
"Snort snort!\""

#: src-en/data/events_sector_showdown.xml.append:152
msgid ""
"data/events_sector_showdown.xml.append$//event[@name=\"FR_TRUE_VICTORY_CREW_CYBERPONY\"]/choice/text"
