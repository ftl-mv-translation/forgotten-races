msgid ""
"data/events_fr_sector_tartarus.xml$//event[@name=\"FR_WEAPON_CHAOS_SELECT_TARTARUS\"]/choice[3]/text"
msgstr "Choisissez le canon à clones de Pok."

#: src-en/data/events_fr_sector_tartarus.xml:1739
msgid ""
